	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"next",
			"next/core-web-vitals",
			"prettier"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not ie <= 11",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint --cache --cache-location .next/cache/eslint/",
			"npm run prettier"
		],
		"*.{css,scss}": [
			"npm run stylelint"
		]
	}
